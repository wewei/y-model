{
  "name": "y-model",
  "version": "0.0.0",
  "private": true,
  "description": "Create data model with Yjs.",
  "keywords": [
    "y-model",
    "node",
    "typescript"
  ],
  "homepage": "https://github.com/wewei/y-model",
  "repository": {
    "type": "git",
    "url": "https://github.com/wewei/y-model"
  },
  "license": "MIT",
  "author": {
    "name": "Wei Wei",
    "email": "shazhouke@gmail.com",
    "url": ""
  },
  "files": [
    "dist/*.*js.map",
    "dist/*.*js",
    "dist/*.d.ts.map",
    "dist/*.d.ts"
  ],
  "main": "dist/index",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "clean": "shx rm -rf dist/ .*cache *.log",
    "lint": "npm run lint:debug -- --config tslint.prod.json",
    "lint:debug": "tslint --project tsconfig.json --format stylish",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "shx echo 'Write your own tests'",
    "ts": "tsc",
    "watch": "tsc --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && tsc --noEmit"
    }
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "yjs": "^13.5.48"
  },
  "devDependencies": {
    "@reallyland/tsconfig": "^2.0.0",
    "@reallyland/tslint-config": "^1.1.1",
    "@types/node": "^13.9.8",
    "@types/uuid": "^9.0.1",
    "husky": "^4.2.3",
    "shx": "^0.3.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
